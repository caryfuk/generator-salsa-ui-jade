doctype html
html(lang="en")

  block vars

  head
    block title
      title #{pageTitle} | #{siteName}
    block includes
      link(rel='stylesheet' type='text/css' href='/styles/style.css')

  body
    block header
      h1 #{pageTitle}
    block content
    block footer
      footer
        p
          a(href="/") #{siteName}
